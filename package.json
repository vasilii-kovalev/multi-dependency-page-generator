{
  "name": "multi-dependency-page-generator",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "tslint": "tsc",
    "eslint:check": "eslint --ext .ts,.tsx src --color",
    "eslint:fix": "yarn eslint:check --fix",
    "stylelint:check": "stylelint src/**/*.scss --color",
    "stylelint:fix": "yarn stylelint:check --fix",
    "lint": "yarn tslint && yarn eslint:check && yarn stylelint:check",
    "build": "yarn tslint && vite build",
    "serve": "vite preview",
    "formatter:check": "prettier . --check",
    "formatter:fix": "prettier . --write"
  },
  "dependencies": {
    "@csstools/normalize.css": "^11.0.1",
    "antd": "^4.16.2",
    "axios": "^0.21.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-query": "^3.17.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "msw": "^0.29.0",
    "prettier": "^2.3.1",
    "sass": "^1.35.1",
    "stylelint": "^13.13.1",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "typescript": "^4.3.2",
    "vite": "^2.3.7"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
